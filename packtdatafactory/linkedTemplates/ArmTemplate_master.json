{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "packtdatafactory"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSqlDatabaseLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLinkedService'"
		},
		"CosmosDbLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbLinkedService'"
		},
		"DataLakeStoreLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeStoreLinkedService'"
		},
		"HDInsightLinkedService_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'HDInsightLinkedService'"
		},
		"HDInsightLinkedService_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'HDInsightLinkedService'"
		},
		"HDInsightLinkedService_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'HDInsightLinkedService'"
		},
		"PacktStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PacktStorage'"
		},
		"SynapseSQLPoolLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SynapseSQLPoolLinkedService'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://packtdatalakestore.dfs.core.windows.net"
		},
		"DataLakeStoreLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://packtdatalakestore.dfs.core.windows.net"
		},
		"HDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "b85b0984-a391-4f22-a832-fb6e46c39f38"
		},
		"HDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "packtade"
		},
		"HDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "576de443-07c3-49ed-af47-ee7ecfd0d16c"
		},
		"HDInsightLinkedService_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "8a4925a9-fd8e-4866-b31c-f719fb05dce6"
		},
		"HDInsightLinkedService_properties_typeProperties_clusterUserName": {
			"type": "string",
			"defaultValue": "sshadmin"
		},
		"HDInsightLinkedService_properties_typeProperties_clusterSshUserName": {
			"type": "string",
			"defaultValue": "sshadmin"
		},
		"NewFileTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b85b0984-a391-4f22-a832-fb6e46c39f38/resourceGroups/packtade/providers/Microsoft.Storage/storageAccounts/packtdatalakestore"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b85b0984-a391-4f22-a832-fb6e46c39f38/resourceGroups/packtade/providers/Microsoft.Storage/storageAccounts/packtdatalakestore"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlDatabaseLinkedService_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLinkedService_connectionString')]"
					},
					"CosmosDbLinkedService_connectionString": {
						"value": "[parameters('CosmosDbLinkedService_connectionString')]"
					},
					"DataLakeStoreLinkedService_accountKey": {
						"value": "[parameters('DataLakeStoreLinkedService_accountKey')]"
					},
					"PacktStorage_connectionString": {
						"value": "[parameters('PacktStorage_connectionString')]"
					},
					"SynapseSQLPoolLinkedService_connectionString": {
						"value": "[parameters('SynapseSQLPoolLinkedService_connectionString')]"
					},
					"HDInsightLinkedService_clusterPassword": {
						"value": "[parameters('HDInsightLinkedService_clusterPassword')]"
					},
					"HDInsightLinkedService_clusterSshPassword": {
						"value": "[parameters('HDInsightLinkedService_clusterSshPassword')]"
					},
					"HDInsightLinkedService_servicePrincipalKey": {
						"value": "[parameters('HDInsightLinkedService_servicePrincipalKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"DataLakeStoreLinkedService_properties_typeProperties_url": {
						"value": "[parameters('DataLakeStoreLinkedService_properties_typeProperties_url')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					},
					"HDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
						"value": "[parameters('HDInsightLinkedService_properties_typeProperties_hostSubscriptionId')]"
					},
					"HDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('HDInsightLinkedService_properties_typeProperties_clusterResourceGroup')]"
					},
					"HDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('HDInsightLinkedService_properties_typeProperties_servicePrincipalId')]"
					},
					"HDInsightLinkedService_properties_typeProperties_tenant": {
						"value": "[parameters('HDInsightLinkedService_properties_typeProperties_tenant')]"
					},
					"HDInsightLinkedService_properties_typeProperties_clusterUserName": {
						"value": "[parameters('HDInsightLinkedService_properties_typeProperties_clusterUserName')]"
					},
					"HDInsightLinkedService_properties_typeProperties_clusterSshUserName": {
						"value": "[parameters('HDInsightLinkedService_properties_typeProperties_clusterSshUserName')]"
					},
					"NewFileTrigger_properties_typeProperties_scope": {
						"value": "[parameters('NewFileTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}